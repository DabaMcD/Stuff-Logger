<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.stuff.log.ger.LogActivity"
    android:id="@+id/LayoutBackground">

    <com.stuff.log.ger.LogTopBarView
        android:id="@+id/logTopBarView"
        android:layout_height="match_parent"
        android:layout_width="match_parent" />
    <com.stuff.log.ger.LogView
        android:id="@+id/logView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <com.stuff.log.ger.NewLoglineButtonView
        android:id="@+id/newLoglineButton"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <com.stuff.log.ger.ClearLogButtonView
        android:id="@+id/clearLogButtonView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</android.support.constraint.ConstraintLayout>
